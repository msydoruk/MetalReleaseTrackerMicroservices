# Environment Configuration Template for Shared Infrastructure
# Copy this file to .env and fill in your actual values

# Server Configuration
PUBLIC_IP=localhost

# Seq Logging (Centralized distributed logging)
SEQ_ADMIN_PASSWORD=your_secure_seq_password

# MinIO Object Storage (Shared across services)
# Note: Individual services create their own buckets using these credentials
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=your_secure_minio_password
