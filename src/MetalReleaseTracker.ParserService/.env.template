# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Server Configuration
PUBLIC_IP=your_server_ip_here

# PostgreSQL Database
POSTGRES_USER=your_postgres_username
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_DB=your_database_name

# MinIO Object Storage
MINIO_ROOT_USER=your_minio_username
MINIO_ROOT_PASSWORD=your_secure_minio_password
MINIO_BUCKET_NAME=metal-release-tracker

# TickerQ Dashboard
TICKERQ_DASHBOARD_USERNAME=your_dashboard_username
TICKERQ_DASHBOARD_PASSWORD=your_secure_dashboard_password

# Seq Logging
SEQ_ADMIN_PASSWORD=your_secure_seq_password